CompileFlags:
  # The compile_commands.json has already been translated to Clang-compatible flags
  # by tools/fix_compile_commands.py
  # Include order: Clang builtins first, then TI headers for libc functions

  Add:
    - -std=c11
    - -Wno-unknown-warning-option
    - -Wno-unknown-pragmas

Diagnostics:
  # Suppress diagnostics that don't apply to embedded systems
  UnusedIncludes: None
  MissingIncludes: None
  ClangTidy:
    Remove:
      - bugprone-reserved-identifier
      - readability-identifier-naming
